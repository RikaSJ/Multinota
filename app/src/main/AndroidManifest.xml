<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoenix.multinotas">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

        <activity android:name="com.example.phoenix.multinotas.Datos.ActivityDatos"></activity>
        <activity android:name="com.example.phoenix.multinotas.Datos.ActivityNotifica"></activity>
        <activity android:name="com.example.phoenix.multinotas.Media.ActivityMedia"></activity>
        <service android:name="com.example.phoenix.multinotas.Servicio"/>

    </application>


</manifest>
